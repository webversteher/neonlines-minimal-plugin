{
  "name": "vendor/neonlines-minimal-plugin",
  "description": "Minimal Plugin mit Minor-Versionserkennung",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "version": "0.0.1",
  "require": {
    "shopware/core": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Webversteher\\NeonlinesMinimalPlugin\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "Webversteher\\NeonlinesMinimalPlugin\\NeonlinesMinimalPlugin",
    "label": {
      "de-DE": "Minimal Plugin",
      "en-GB": "Minimal Plugin"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}